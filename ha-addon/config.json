{
  "name": "cs2mqtt",
  "version": "1.8.0",
  "slug": "cs2mqtt",
  "description": "Counter-Strike 2 Game State Integration to MQTT bridge",
  "url": "https://github.com/lupusbytes/cs2mqtt",
  "startup": "application",
  "services": [
    "mqtt:need"
  ],
  "hassio_api": true,
  "panel_icon": "mdi:pistol",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "timeout": 30,
  "legacy": true,
  "ports": {
    "8080/tcp": 5000
  },
  "ports_description": {
    "8080/tcp": "Game State API port"
  },
  "options": {
    "MQTT__Host": "homeassistant.local",
    "MQTT__Port": 1883,
    "MQTT__UseTLS": false,
    "MQTT__Username": "",
    "MQTT__Password": "",
    "MQTT__ClientId": "",
    "MQTT__ProtocolVersion": "5.0.0"
  },
  "schema": {
    "MQTT__Host": "str",
    "MQTT__Port": "int",
    "MQTT__UseTLS": "bool",
    "MQTT__Username": "str?",
    "MQTT__Password": "password?",
    "MQTT__ClientId": "str?",
    "MQTT__ProtocolVersion": "list(5.0.0|3.1.1|3.1.0)"
  },
  "image": "ghcr.io/lupusbytes/cs2mqtt"
}
