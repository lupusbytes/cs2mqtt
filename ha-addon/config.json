{
  "name": "cs2mqtt-dev",
  "version": "main",
  "slug": "cs2mqtt-dev",
  "description": "Counter-Strike 2 Game State Integration to MQTT bridge",
  "url": "https://github.com/davidramiro/cs2mqtt",
  "startup": "application",
  "services": [
    "mqtt:need"
  ],
  "hassio_api": true,
  "panel_icon": "mdi:pistol",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "timeout": 30,
  "legacy": true,
  "ports": {
    "8080/tcp": 5000
  },
  "ports_description": {
    "8080/tcp": "Game State API port"
  },
  "options": {
  },
  "schema": {
    "MQTT__Host": "str?",
    "MQTT__Port": "int?",
    "MQTT__UseTLS": "bool?",
    "MQTT__Username": "str?",
    "MQTT__Password": "password?",
    "MQTT__ClientId": "str?",
    "MQTT__ProtocolVersion": "list(5.0.0|3.1.1|3.1.0)?"
  },
  "image": "ghcr.io/davidramiro/cs2mqtt"
}
